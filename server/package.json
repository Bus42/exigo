{
  "private": true,
  "name": "exigo-server",
  "version": "0.0.0",
  "scripts": {
    "build": "netlify-lambda build src",
    "clean": "del-cli \"**/dev.db\"",
    "develop": "run-p develop:*",
    "develop:apollo": "netlify-lambda serve src",
    "develop:prisma": "prisma2 dev",
    "generate": "run-s generate:prisma generate:nexus",
    "generate:nexus": "ts-node --transpile-only src/schema",
    "generate:prisma": "prisma2 generate && prisma2 lift up --create-db",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "apollo-server": "^2.9.9",
    "apollo-server-lambda": "^2.9.9",
    "graphql": "^14.5.8",
    "nexus": "^0.12.0-rc.4",
    "nexus-prisma": "^0.5.2",
    "prisma2": "^2.0.0-preview016.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.7.2",
    "del-cli": "^3.0.0",
    "netlify-lambda": "^1.6.3",
    "npm-run-all": "^4.1.5",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  }
}
